{"ast":null,"code":"import * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\"; // import ContactService from \"@/services/contact.service\";\n\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage\n  },\n  props: {\n    id: {\n      type: String,\n      required: true\n    },\n    products: {\n      type: Object,\n      require: true\n    }\n  },\n\n  data() {\n    const productFormSchema = yup.object().shape({\n      name: yup.string().required(\"Tên phải có giá trị.\").min(2, \"Tên phải ít nhất 2 ký tự.\").max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      img: yup.array(),\n      quantity: yup.string().required(\"Tên phải có giá trị.\"),\n      BaPrice: yup.string().required(\"Không được để trống trường này.\").min(4, \"Giá của sản phẩm phải tối thiểu 4 chữ số.\").max(50, \"E-mail tối đa 50 ký tự.\"),\n      PrPrice: yup.string().required(\"Không được để trống trường này.\").min(4, \"Giá của sản phẩm phải tối thiểu 4 chữ số.\").max(50, \"E-mail tối đa 50 ký tự.\")\n    });\n    return {\n      //   product: {\n      //     type: Object,\n      //     require: true,\n      //   },\n      // productLocal: this.product,\n      product: this.products,\n      productFormSchema\n    };\n  },\n\n  methods: {\n    test() {\n      console.log(this.id);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2EA,OAAO,KAAKA,GAAZ,MAAqB,KAArB;AACA,SAASC,IAAT,EAAeC,KAAf,EAAsBC,YAAtB,QAA0C,cAA1C,C,CACA;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,IADU;IAEVC,KAFU;IAGVC;EAHU,CADC;EAMbE,KAAK,EAAE;IACLC,EAAE,EAAE;MAAEC,IAAI,EAAEC,MAAR;MAAgBC,QAAQ,EAAE;IAA1B,CADC;IAELC,QAAQ,EAAE;MACRH,IAAI,EAAEI,MADE;MAERC,OAAO,EAAE;IAFD;EAFL,CANM;;EAabC,IAAI,GAAG;IACL,MAAMC,iBAAgB,GAAId,GAAG,CAACe,MAAJ,GAAaC,KAAb,CAAmB;MAC3CC,IAAI,EAAEjB,GAAE,CACLkB,MADG,GAEHT,QAFG,CAEM,sBAFN,EAGHU,GAHG,CAGC,CAHD,EAGI,2BAHJ,EAIHC,GAJG,CAIC,GAJD,EAIM,8BAJN,CADqC;MAM3CC,GAAG,EAAErB,GAAG,CAACsB,KAAJ,EANsC;MAO3CC,QAAQ,EAAEvB,GAAG,CAACkB,MAAJ,GAAaT,QAAb,CAAsB,sBAAtB,CAPiC;MAQ3Ce,OAA