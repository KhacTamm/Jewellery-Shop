{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"pinia\";\nimport { useAppStore } from \"@/stores/app.store\";\nimport AdminService from \"@/services/admin.service\";\nexport default {\n  data() {\n    return {\n      products: [],\n      checked: false,\n      courseItemCheckbox: [],\n      id: \"\"\n    };\n  },\n\n  computed: {\n    filteredProducts() {\n      return this.products;\n    },\n\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    }\n\n  },\n  methods: { ...mapActions(useAppStore, [\"formatter\"]),\n\n    async deleteAllProduct() {\n      try {\n        this.$router.push({\n          name: \"AdminAllProduct\"\n        });\n        await AdminService.deleteAll();\n        this.$refs.form.$el.submit();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async deleteProduct(id) {\n      try {\n        await AdminService.forceDelete(id);\n        this.$router.push({\n          name: \"AdminAllProduct\"\n        });\n        this.$refs.form.$el.submit();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async retrieveProducts() {\n      try {\n        this.products = await AdminService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n\n  mounted() {\n    this.retrieveProducts();\n  }\n\n};","map":{"version":3,"mappings":";AA4HA,SAASA,UAAT,QAA2B,OAA3B;AACA,SAASC,WAAT,QAA4B,oBAA5B;AACA,OAAOC,YAAP,MAAyB,0BAAzB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,OAAO,EAAE,KAFJ;MAGLC,kBAAkB,EAAE,EAHf;MAILC,EAAE,EAAE;IAJC,CAAP;EAMD,CARY;;EASbC,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,OAAO,KAAKL,QAAZ;IACD,CAHO;;IAIRM,qBAAqB,GAAG;MACtB,OAAO,KAAKD,gBAAL,CAAsBE,MAA7B;IACD;;EANO,CATG;EAiBbC,OAAO,EAAE,EACP,GAAGZ,UAAU,CAACC,WAAD,EAAc,CAAC,WAAD,CAAd,CADN;;IAGP,MAAMY,gBAAN,GAAyB;MACvB,IAAI;QACF,KAAKC,OAAL,CAAaC,IAAb,CAAkB;UAAEC,IAAI,EAAE;QAAR,CAAlB;QACA,MAAMd,YAAY,CAACe,SAAb,EAAN;QACA,KAAKC,KAAL,CAAWC,