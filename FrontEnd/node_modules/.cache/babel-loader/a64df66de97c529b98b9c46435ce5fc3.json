>\n        <!-- <input type=\"file\" class=\"form-control\" id=\"img\" name=\"img\" />\n        <input type=\"file\" class=\"form-control\" id=\"img\" name=\"img\" />\n        <input type=\"file\" class=\"form-control\" id=\"img\" name=\"img\" /> -->\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"color\" class=\"form-label\">Giá Gốc</label>\n        <Field\n          id=\"BaPrice\"\n          name=\"BaPrice\"\n          type=\"text\"\n          class=\"form-control\"\n          v-model=\"productLocal.BaPrice\"\n        />\n        <ErrorMessage name=\"price\" class=\"error-feedback\" />\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"color\" class=\"form-label\">Giá Khuyến Mãi</label>\n        <Field\n          id=\"PrPrice\"\n          name=\"PrPrice\"\n          type=\"text\"\n          class=\"form-control\"\n          v-model=\"productLocal.PrPrice\"\n        />\n        <ErrorMessage name=\"price\" class=\"error-feedback\" />\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"description\" class=\"form-label\">Mô tả khóa học</label>\n        <Field\n          id=\"description\"\n          name=\"description\"\n          type=\"text\"\n          class=\"form-control\"\n          v-model=\"productLocal.description\"\n        />\n        <ErrorMessage name=\"description\" class=\"error-feedback\" />\n      </div>\n      <!-- <div class=\"mb-3\">\n        <label for=\"color\" class=\"form-label\">Màu</label>\n        <input type=\"text\" class=\"form-control\" id=\"color\" name=\"color\" />\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"Size\" class=\"form-label\">Size</label>\n        <input type=\"text\" class=\"form-control\" id=\"size\" name=\"size\" />\n      </div> -->\n      <button type=\"submit\" class=\"btn btn-primary\">Lưu</button>\n    </Form>\n  </div>\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  props: {\n    product: { type: Object, required: true },\n  },\n  data() {\n    const productFormSchema = yup.object().shape({\n      name: yup\n        .string()\n        .required(\"Tên phải có giá trị.\")\n        .min(2, \"Tên phải ít nhất 2 ký tự.\")\n        .max(50, \"Tên có nhiều nhất 50 ký tự.\"),\n      email: yup\n        .string()\n        .email(\"E-mail không đúng.\")\n        .max(50, \"E-mail tối đa 50 ký tự.\"),\n      address: yup.string().max(100, \"Địa chỉ tối đa 100 ký tự.\"),\n      phone: yup\n        .string()\n        .matches(\n          /((09|03|07|08|05)+([0-9]{8})\\b)/g,\n          \"Số điện thoại không hợp lệ.\"\n        ),\n    });\n    return {\n      // Chúng ta sẽ không muốn hiệu chỉnh props, nên tạo biến cục bộ\n      // để liên kết với các input trên form\n      productLocal: this.product,\n      productFormSchema,\n      file: [],\n    };\n  },\n  methods: {\n    handleFileUpload() {\n      this.file.push(this.$refs.file.files[0].name);\n      this.productLocal.img = this.file;\n      console.log(this.file);\n      console.log(this.productLocal);\n      console.log(this.productLocal.img);\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}