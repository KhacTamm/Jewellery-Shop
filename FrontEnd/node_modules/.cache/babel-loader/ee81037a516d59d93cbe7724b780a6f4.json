{"ast":null,"code":"import * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage\n  },\n\n  data() {\n    const productFormSchema = yup.object().shape({\n      name: yup.string().required(\"Vui lòng nhập trường này.\").min(2, \"Vui lòng tối thiểu 2 kí tự.\").max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      phone: yup.string().matches(/((09|03|07|08|05)+([0-9]{8})\\b)/g, \"Số điện thoại không hợp lệ.\"),\n      password: yup.string().min(4, \"Vui lòng nhập tối thiểu 4 kí tự\").max(8, \"Mật khẩu nhiều nhất 8 ký tự.\")\n    });\n    return {\n      productFormSchema\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkDA,OAAO,KAAKA,GAAZ,MAAqB,KAArB;AACA,SAASC,IAAT,EAAeC,KAAf,EAAsBC,YAAtB,QAA0C,cAA1C;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,IADU;IAEVC,KAFU;IAGVC;EAHU,CADC;;EAMbE,IAAI,GAAG;IACL,MAAMC,iBAAgB,GAAIN,GAAG,CAACO,MAAJ,GAAaC,KAAb,CAAmB;MAC3CC,IAAI,EAAET,GAAE,CACLU,MADG,GAEHC,QAFG,CAEM,2BAFN,EAGHC,GAHG,CAGC,CAHD,EAGI,6BAHJ,EAIHC,GAJG,CAIC,GAJD,EAIM,8BAJN,CADqC;MAM3CC,KAAK,EAAEd,GAAE,CACNU,MADI,GAEJK,OAFI,CAGH,kCAHG,EAIH,6BAJG,CANoC;MAY3CC,QAAQ,EAAEhB,GAAE,CACTU,MADO,GAEPE,GAFO,CAEH,CAFG,EAEA,iCAFA,EAGPC,GAHO,CAGH,CAHG,EAGA,8BAHA;IAZiC,CAAnB,CAA1B;IAiBA,OAAO;MACLP;IADK,CAAP;EAGD;;AA3BY,CAAf","names":["yup","Form","Field","ErrorMessage","components","data","productFormSchema","object","shape","name","string","required","min","max","phone","matches","password"],"sourceRoot":"","sources":["D:\\CT449\\shoponline\\src\\components\\AppRegister.vue"],"sourcesContent":["<template>\n  <div id=\"login\">\n    <h2 class=\"titleLogin\">Tạo tài khoản</h2>\n    <Form class=\"login\" :validation-schema=\"productFormSchema\">\n      <ul>\n        <li>\n          <Field\n            name=\"username\"\n            type=\"text\"\n            placeholder=\"Tên tài khoản\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"username\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <Field\n            name=\"phone\"\n            type=\"text\"\n            placeholder=\"Số điện thoại\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"phone\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <Field\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Mật khẩu\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"password\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <Field\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Nhập lại mật khẩu\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"password\" class=\"error-feedback\" />\n        </li>\n        <div class=\"form-login\">\n          <button type=\"submit\" class=\"login btn\">ĐĂNG KÝ</button>\n        </div>\n      </ul>\n    </Form>\n  </div>\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\n\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  data() {\n    const productFormSchema = yup.object().shape({\n      name: yup\n        .string()\n        .required(\"Vui lòng nhập trường này.\")\n        .min(2, \"Vui lòng tối thiểu 2 kí tự.\")\n        .max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      phone: yup\n        .string()\n        .matches(\n          /((09|03|07|08|05)+([0-9]{8})\\b)/g,\n          \"Số điện thoại không hợp lệ.\"\n        ),\n      password: yup\n        .string()\n        .min(4, \"Vui lòng nhập tối thiểu 4 kí tự\")\n        .max(8, \"Mật khẩu nhiều nhất 8 ký tự.\"),\n    });\n    return {\n      productFormSchema,\n    };\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}