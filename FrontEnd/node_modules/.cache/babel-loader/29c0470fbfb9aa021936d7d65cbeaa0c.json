{"ast":null,"code":"import { useProductData } from \"@/stores/Products\";\nimport AccountService from \"@/services/account.service\";\nexport default {\n  setup() {\n    const stores = useProductData();\n    return {\n      stores\n    };\n  },\n\n  data() {\n    return {\n      Logo: require(\"../assets/img/imgLocal/logo.png\"),\n      isdisplay: false,\n      data: {},\n      dataUser: useProductData()\n    };\n  },\n\n  methods: {\n    displayMenuAccount() {\n      this.isdisplay = !this.isdisplay;\n    },\n\n    async setUpAccount() {\n      try {\n        console.log(this.stores.getUsetName);\n        this.data = await AccountService.getAccount(); // console.log(this.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async dx() {\n      await this.logout.Logout();\n    }\n\n  },\n\n  created() {\n    this.setUpAccount();\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,SAASA,cAAT,QAA+B,mBAA/B;AACA,OAAOC,cAAP,MAA2B,4BAA3B;AAEA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIH,cAAc,EAA7B;IACA,OAAO;MAAEG;IAAF,CAAP;EACD,CAJY;;EAKbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAEC,OAAO,CAAC,iCAAD,CADR;MAELC,SAAS,EAAE,KAFN;MAGLH,IAAI,EAAE,EAHD;MAILI,QAAQ,EAAER,cAAc;IAJnB,CAAP;EAMD,CAZY;;EAabS,OAAO,EAAE;IACPC,kBAAkB,GAAG;MACnB,KAAKH,SAAL,GAAiB,CAAC,KAAKA,SAAvB;IACD,CAHM;;IAIP,MAAMI,YAAN,GAAqB;MACnB,IAAI;QACFC,OAAO,CAACC,GAAR,CAAY,KAAKV,MAAL,CAAYW,WAAxB;QACA,KAAKV,IAAL,GAAY,MAAMH,cAAc,CAACc,UAAf,EAAlB,CAFE,CAGF;MACF,CAJA,CAIE,OAAOC,KAAP,EAAc;QACdJ,OAAO,CAACC,GAAR,CAAYG,KAAZ;MACF;IACD,CAZM;;IAaP,MAAMC,EAAN,GAAW;MACT,MAAM,KAAKC,MAAL,CAAYC,MAAZ,EAAN;IACD;;EAfM,CAbI;;EA8BbC,OAAO,GAAG;IACR,KAAKT,YAAL;EACD;;AAhCY,CAAf","names":["useProductData","AccountService","setup","stores","data","Logo","require","isdisplay","dataUser","methods","displayMenuAccount","setUpAccount","console","log","getUsetName","getAccount","error","dx","logout","Logout","created"],"sourceRoot":"","sources":["D:\\CT449\\shoponline\\src\\components\\AppHeader.vue"],"sourcesContent":["<template>\n  <header class=\"header\">\n    <img :src=\"Logo\" alt=\"\" />\n    <div class=\"tkh\" :class=\"{ ds: isdisplay }\">\n      <div class=\"tkcdn\">\n        <router-link to=\"/register\" class=\"dk tk\">Đăng Ký</router-link>\n        <router-link to=\"/login\" class=\"tk\" @click=\"setUpAccount\"\n          >Đăng Nhập</router-link\n        >\n      </div>\n      <div class=\"tkddn\">\n        <!-- <div class=\"accountName\">{{ data.dataUsers }}</div> -->\n        <div class=\"accountName\">{{ data.username }}</div>\n        <div class=\"btn\" @click=\"displayMenuAccount\">\n          <i class=\"fas fa-caret-down btn-ac\"></i>\n          <i class=\"fas fa-caret-up btn-ac\"></i>\n          <ul class=\"content__account\">\n            <li class=\"item_account\">Tài Khoản</li>\n            <li class=\"item_account\" @click=\"dx\">Đăng Xuất</li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  </header>\n</template>\n\n<script>\nimport { useProductData } from \"@/stores/Products\";\nimport AccountService from \"@/services/account.service\";\n\nexport default {\n  setup() {\n    const stores = useProductData();\n    return { stores };\n  },\n  data() {\n    return {\n      Logo: require(\"../assets/img/imgLocal/logo.png\"),\n      isdisplay: false,\n      data: {},\n      dataUser: useProductData(),\n    };\n  },\n  methods: {\n    displayMenuAccount() {\n      this.isdisplay = !this.isdisplay;\n    },\n    async setUpAccount() {\n      try {\n        console.log(this.stores.getUsetName);\n        this.data = await AccountService.getAccount();\n        // console.log(this.data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async dx() {\n      await this.logout.Logout();\n    },\n  },\n  created() {\n    this.setUpAccount();\n  },\n};\n</script>\n\n<style scoped>\n.header img {\n  width: 100%;\n  height: 300px;\n}\n\n.tkh {\n  position: absolute;\n  right: 175px;\n  top: 10px;\n  font-weight: 500;\n  font-size: 1.1rem;\n}\n\n.tkcdn {\n  /* display: none; */\n}\n\n.tk {\n  text-decoration: none;\n  color: #fff;\n  padding: 0px 8px;\n}\n\n.tk:hover {\n  color: #000;\n}\n\n.dk {\n  border-right: #dddddd 1px solid;\n}\n\n.tkddn {\n  display: flex;\n  color: #fff;\n  text-shadow: 2px 2px 2px var(--hover);\n  /* display: none; */\n}\n\n.fas {\n  font-size: 1.2rem;\n  cursor: pointer;\n  color: #fff;\n}\n\n.tkh .fa-caret-up {\n  display: none;\n}\n\n.tkh.ds .fa-caret-up {\n  display: inline-block;\n}\n\n.tkh.ds .fa-caret-down {\n  display: none;\n}\n\n.content__account {\n  position: absolute;\n  background-color: rgba(154, 181, 200, 0.8);\n  right: -8px;\n  top: 95%;\n  display: none;\n  cursor: pointer;\n  color: #fff;\n  text-align: center;\n  text-shadow: none;\n  padding: 6px 0;\n}\n\n.item_account {\n  padding: 8px 30px;\n  font-weight: 650;\n}\n\n.item_account:hover {\n  background-color: rgba(211, 211, 211, 0.4);\n  color: #000;\n}\n\n.tkh.ds .content__account {\n  display: block;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}