/div>\n      <ErrorMessage name=\"img\" class=\"error-feedback\" />\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"color\" class=\"form-label\">Giá Gốc</label>\n      <Field\n        id=\"BaPrice\"\n        name=\"BaPrice\"\n        type=\"text\"\n        class=\"form-control\"\n        v-model=\"productLocal.BaPrice\"\n      />\n      <ErrorMessage name=\"price\" class=\"error-feedback\" />\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"color\" class=\"form-label\">Giá Khuyến Mãi</label>\n      <Field\n        id=\"PrPrice\"\n        name=\"PrPrice\"\n        type=\"text\"\n        class=\"form-control\"\n        v-model=\"productLocal.PrPrice\"\n      />\n      <ErrorMessage name=\"price\" class=\"error-feedback\" />\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"description\" class=\"form-label\">Size</label>\n      <div class=\"categorySiz\">\n        \n        <input name=\"size\" type=\"checkbox\" class=\"\" />\n      </div>\n      <ErrorMessage name=\"description\" class=\"error-feedback\" />\n    </div>\n    <div class=\"mb-3\">\n      <label for=\"description\" class=\"form-label\">Mô tả sản phẩm</label>\n      <Field\n        id=\"description\"\n        name=\"description\"\n        type=\"text\"\n        class=\"form-control\"\n        v-model=\"productLocal.description\"\n      />\n      <ErrorMessage name=\"description\" class=\"error-feedback\" />\n    </div>\n    <div class=\"btnSM\">\n      <button type=\"submit\" class=\"btn btn-primary\">Lưu</button>\n    </div>\n  </Form>\n  <!-- </div> -->\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  props: {\n    product: { type: Object, required: true },\n  },\n  data() {\n    const productFormSchema = yup.object().shape({\n      name: yup\n        .string()\n        .required(\"Tên phải có giá trị.\")\n        .min(2, \"Tên phải ít nhất 2 ký tự.\")\n        .max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      img: yup.array(),\n      price: yup\n        .string()\n        .email(\"Không được để trống trường này.\")\n        .min(2, \"Tên phải ít nhất 2 ký tự.\")\n        .max(50, \"E-mail tối đa 50 ký tự.\"),\n    });\n    return {\n      // Chúng ta sẽ không muốn hiệu chỉnh props, nên tạo biến cục bộ\n      // để liên kết với các input trên form\n      productLocal: this.product,\n      productFormSchema,\n      file: [],\n      previousNameFile: [],\n    };\n  },\n  methods: {\n    handleFileUpload() {\n      this.file.push(this.$refs.file.files[0].name);\n      this.productLocal.img = this.file;\n      this.previousNameFile = this.file;\n      console.log(this.file);\n      console.log(this.productLocal);\n      console.log(this.previousNameFile);\n      console.log(this.previousNameFile.length);\n    },\n  },\n};\n</script>\n\n<style cope>\n.sumImg {\n  padding: 12px 0;\n}\n\n.FileNameItem {\n  color: #000;\n  padding: 6px 0;\n}\n.error-feedback {\n  color: rgb(255, 0, 0);\n}\n\n.btnSM {\n  text-align: right;\n  padding: 0 12px;\n}\n\n.btn-primary {\n  padding: 12px 24px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}