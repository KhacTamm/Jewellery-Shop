{"ast":null,"code":"import InputSearch from \"./InputSearch.vue\";\nexport default {\n  components: {\n    InputSearch\n  },\n\n  data() {\n    return {\n      products: [],\n      searchText: \"\",\n      activeIndex: -1\n    };\n  },\n\n  watch: {\n    // Giám sát các thay đổi của biến searchText.\n    // Bỏ chọn phần tử đang được chọn trong danh sách.\n    searchText() {\n      this.activeIndex = -1;\n    }\n\n  },\n  computed: {\n    // Chuyển các đối tượng contact thành chuỗi để tiện cho tìm kiếm.\n    productStrings() {\n      return this.contacts.map(product => {\n        const {\n          type,\n          name\n        } = product;\n        return [type, name].join(\"\");\n      });\n    },\n\n    // Trả về các contact có chứa thông tin cần tìm kiếm.\n    filteredContacts() {\n      if (!this.searchText) return this.contacts;\n      return this.contacts.filter((contact, index) => this.contactStrings[index].includes(this.searchText));\n    },\n\n    activeContact() {\n      if (this.activeIndex < 0) return null;\n      return this.contacts[this.activeIndex];\n    }\n\n  },\n  methods: {\n    updateModelValue(e) {\n      this.$emit(\"update:modelValue\", e.target.value);\n    },\n\n    submit() {\n      this.$emit(\"submit\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,WAAP,MAAwB,mBAAxB;AACA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAIbE,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,UAAU,EAAE,EAFP;MAGLC,WAAW,EAAE,CAAC;IAHT,CAAP;EAKD,CAVY;;EAWbC,KAAK,EAAE;IACL;IACA;IACAF,UAAU,GAAG;MACX,KAAKC,WAAL,GAAmB,CAAC,CAApB;IACD;;EALI,CAXM;EAkBbE,QAAQ,EAAE;IACR;IACAC,cAAc,GAAG;MACf,OAAO,KAAKC,QAAL,CAAcC,GAAd,CAAmBC,OAAD,IAAa;QACpC,MAAM;UAAEC,IAAF;UAAQC;QAAR,IAAiBF,OAAvB;QACA,OAAO,CAACC,IAAD,EAAOC,IAAP,EAAaC,IAAb,CAAkB,EAAlB,CAAP;MACD,CAHM,CAAP;IAID,CAPO;;IAQR;IACAC,gBAAgB,GAAG;MACjB,IAAI,CAAC,KAAKX,UAAV,EAAsB,OAAO,KAAKK,QAAZ;MACtB,OAAO,KAAKA,QAAL,CAAcO,MAAd,CAAqB,CAACC,OAAD,EAAUC,KAAV,KAC1B,KAAKC,cAAL,CAAoBD,KAApB,EAA2BE,QAA3B,CAAoC,KAAKhB,UAAzC,CADK,CAAP;IAGD,CAdO;;IAeRiB,aAAa,GAAG;MACd,IAAI,KAAKhB,WAAL,GAAmB,CAAvB,EAA0B,OAAO,IAAP;MAC1B,OAAO,KAAKI,QAAL,CAAc,KAAKJ,WAAnB,CAAP;IACD;;EAlBO,CAlBG;EAsCbiB,OAAO,EAAE;IACPC,gBAAgB,CAACC,CAAD,EAAI;MAClB,KAAKC,KAAL,CAAW,mBAAX,EAAgCD,CAAC,CAACE,MAAF,CAASC,KAAzC;IACD,CAHM;;IAIPC,MAAM,GAAG;MACP,KAAKH,KAAL,CAAW,QAAX;IACD;;EANM;AAtCI,CAAf","names":["InputSearch","components","data","products","searchText","activeIndex","watch","computed","productStrings","contacts","map","product","type","name","join","filteredContacts","filter","contact","index","contactStrings","includes","activeContact","methods","updateModelValue","e","$emit","target","value","submit"],"sourceRoot":"","sources":["D:\\CT449\\shoponline\\src\\components\\AppNavbar.vue"],"sourcesContent":["<template>\n  <nav class=\"navheader\">\n    <div class=\"list\">\n      <router-link to=\"/\" class=\"list-item\">Trang chủ</router-link>\n      <router-link to=\"/about\" class=\"list-item\">Giới thiệu</router-link>\n      <router-link to=\"/product\" class=\"list-item\">Sản phẩm</router-link>\n      <router-link to=\"/cart\" class=\"list-item\"\n        ><i class=\"fa-solid fa-basket-shopping\"></i\n      ></router-link>\n    </div>\n    <div class=\"search\">\n      <input-search v-model=\"searchText\"> </input-search>\n    </div>\n  </nav>\n</template>\n\n<script>\nimport InputSearch from \"./InputSearch.vue\";\nexport default {\n  components: {\n    InputSearch,\n  },\n  data() {\n    return {\n      products: [],\n      searchText: \"\",\n      activeIndex: -1,\n    };\n  },\n  watch: {\n    // Giám sát các thay đổi của biến searchText.\n    // Bỏ chọn phần tử đang được chọn trong danh sách.\n    searchText() {\n      this.activeIndex = -1;\n    },\n  },\n  computed: {\n    // Chuyển các đối tượng contact thành chuỗi để tiện cho tìm kiếm.\n    productStrings() {\n      return this.contacts.map((product) => {\n        const { type, name } = product;\n        return [type, name].join(\"\");\n      });\n    },\n    // Trả về các contact có chứa thông tin cần tìm kiếm.\n    filteredContacts() {\n      if (!this.searchText) return this.contacts;\n      return this.contacts.filter((contact, index) =>\n        this.contactStrings[index].includes(this.searchText)\n      );\n    },\n    activeContact() {\n      if (this.activeIndex < 0) return null;\n      return this.contacts[this.activeIndex];\n    },\n  },\n  methods: {\n    updateModelValue(e) {\n      this.$emit(\"update:modelValue\", e.target.value);\n    },\n    submit() {\n      this.$emit(\"submit\");\n    },\n  },\n};\n</script>\n\n<style scope>\n.navheader {\n  background-color: var(--backg);\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-top: 10px;\n  height: 50px;\n  padding: 0 4px 0 40px;\n}\n.list {\n  width: 55%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.list-item {\n  text-decoration: none;\n  font-weight: 800;\n  font-size: 1.2rem;\n  color: #fff;\n  cursor: pointer;\n}\n\n.list-item:hover {\n  color: var(--hover);\n}\n\n.fa-basket-shopping {\n  font-size: 1.6rem;\n}\n\n.search {\n  position: relative;\n  width: 40%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}