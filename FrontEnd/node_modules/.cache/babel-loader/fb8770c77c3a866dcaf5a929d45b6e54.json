{"ast":null,"code":"import * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage\n  },\n  props: {\n    account: {\n      type: Object,\n      required: true\n    }\n  },\n\n  data() {\n    const accountFormSchema = yup.object().shape({\n      name: yup.string().required(\"Vui lòng nhập trường này.\").min(2, \"Vui lòng tối thiểu 2 kí tự.\").max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      phone: yup.string().matches(/((09|03|07|08|05)+([0-9]{8})\\b)/g, \"Số điện thoại không hợp lệ.\"),\n      password: yup.string().required(\"Please Enter your password\").matches(/^(?=.*[A-Za-z])(?=.*\\d)(?=.*[@$!%*#?&])[A-Za-z\\d@$!%*#?&]{8,}$/, \"Must Contain 8 Characters, One Uppercase, One Lowercase, One Number and one special case Character\"),\n      isConfirmed: yup.string().oneOf([yup.ref(\"password\"), null], \"Passwords must match\")\n    });\n    return {\n      accountLocal: this.account,\n      accountFormSchema\n    };\n  },\n\n  methods: {\n    test() {\n      this.p = this.accountLocal.password;\n      console.log(this.p);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmDA,OAAO,KAAKA,GAAZ,MAAqB,KAArB;AACA,SAASC,IAAT,EAAeC,KAAf,EAAsBC,YAAtB,QAA0C,cAA1C;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,IADU;IAEVC,KAFU;IAGVC;EAHU,CADC;EAMbE,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAR;MAAgBC,QAAQ,EAAE;IAA1B;EADJ,CANM;;EASbC,IAAI,GAAG;IACL,MAAMC,iBAAgB,GAAIX,GAAG,CAACY,MAAJ,GAAaC,KAAb,CAAmB;MAC3CC,IAAI,EAAEd,GAAE,CACLe,MADG,GAEHN,QAFG,CAEM,2BAFN,EAGHO,GAHG,CAGC,CAHD,EAGI,6BAHJ,EAIHC,GAJG,CAIC,GAJD,EAIM,8BAJN,CADqC;MAM3CC,KAAK,EAAElB,GAAE,CACNe,MADI,GAEJI,OAFI,CAGH,kCAHG,EAIH,6BAJG,CANoC;MAY3CC,QAAQ,EAAEpB,GAAE,CACTe,MADO,GAEPN,QAFO,CAEE,4BAFF,EAGPU,OAHO,CAIN,gEAJM,EAKN,oGALM,CAZiC;MAmB3CE,WAAW,EAAErB,GAAE,CACZe,MADU,GAEVO,KAFU,CAEJ,CAACtB,GAAG,CAACuB,GAAJ,CAAQ,UAAR,CAAD,EAAsB,IAAtB,CAFI,EAEyB,sBAFzB;IAnB8B,CAAnB,CAA1B;IAuBA,OAAO;MACLC,YAAY,EAAE,KAAKlB,OADd;MAELK;IAFK,CAAP;EAID,CArCY;;EAsCbc,OAAO,EAAE;IACPC,IAAI,GAAG;MACL,KAAKC,CAAL,GAAS,KAAKH,YAAL,CAAkBJ,QAA3B;MACAQ,OAAO,CAACC,GAAR,CAAY,KAAKF,CAAjB;IACD;;EAJM;AAtCI,CAAf","names":["yup","Form","Field","ErrorMessage","components","props","account","type","Object","required","data","accountFormSchema","object","shape","name","string","min","max","phone","matches","password","isConfirmed","oneOf","ref","accountLocal","methods","test","p","console","log"],"sourceRoot":"","sources":["D:\\CT449\\shoponline\\src\\components\\AppRegister.vue"],"sourcesContent":["<template>\n  <div id=\"login\">\n    <h2 class=\"titleLogin\">Tạo tài khoản</h2>\n    <Form class=\"login\" :validation-schema=\"accountFormSchema\">\n      <ul>\n        <li>\n          <Field\n            name=\"username\"\n            type=\"text\"\n            placeholder=\"Tên tài khoản\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"username\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <Field\n            name=\"phone\"\n            type=\"text\"\n            placeholder=\"Số điện thoại\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"phone\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <input\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Mật khẩu\"\n            class=\"text\"\n            v-model=\"accountLocal.password\"\n          />\n          <ErrorMessage name=\"password\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <input\n            name=\"isConfirmed\"\n            type=\"password\"\n            placeholder=\"Nhập lại mật khẩu\"\n            class=\"text\"\n          />\n          <ErrorMessage name=\"isConfirmed\" class=\"error-feedback\" />\n        </li>\n        <div class=\"form-login\">\n          <button type=\"submit\" class=\"login btn\" @click=\"test\">ĐĂNG KÝ</button>\n        </div>\n      </ul>\n    </Form>\n  </div>\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\n\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  props: {\n    account: { type: Object, required: true },\n  },\n  data() {\n    const accountFormSchema = yup.object().shape({\n      name: yup\n        .string()\n        .required(\"Vui lòng nhập trường này.\")\n        .min(2, \"Vui lòng tối thiểu 2 kí tự.\")\n        .max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      phone: yup\n        .string()\n        .matches(\n          /((09|03|07|08|05)+([0-9]{8})\\b)/g,\n          \"Số điện thoại không hợp lệ.\"\n        ),\n      password: yup\n        .string()\n        .required(\"Please Enter your password\")\n        .matches(\n          /^(?=.*[A-Za-z])(?=.*\\d)(?=.*[@$!%*#?&])[A-Za-z\\d@$!%*#?&]{8,}$/,\n          \"Must Contain 8 Characters, One Uppercase, One Lowercase, One Number and one special case Character\"\n        ),\n      isConfirmed: yup\n        .string()\n        .oneOf([yup.ref(\"password\"), null], \"Passwords must match\"),\n    });\n    return {\n      accountLocal: this.account,\n      accountFormSchema,\n    };\n  },\n  methods: {\n    test() {\n      this.p = this.accountLocal.password;\n      console.log(this.p);\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}