{"ast":null,"code":"import Products from \"../components/ProductWape.vue\";\nimport ProductService from \"@/services/product.service\";\nimport { mapState, mapActions } from \"pinia\";\nimport { useAppStore } from \"@/stores/app.store\";\nexport default {\n  components: {\n    Products\n  },\n\n  data() {\n    return {\n      products: []\n    };\n  },\n\n  computed: {\n    filteredProducts() {\n      return this.products;\n    },\n\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    },\n\n    ...mapState(useAppStore, {\n      products: \"products\"\n    })\n  },\n  methods: { ...mapActions(useAppStore, [\"retrieveProducts\"]),\n\n    async retrieveProducts() {\n      try {\n        this.products = await ProductService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n\n  mounted() {\n    this.retrieveProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,OAAOA,QAAP,MAAqB,+BAArB;AACA,OAAOC,cAAP,MAA2B,4BAA3B;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAqC,OAArC;AACA,SAASC,WAAT,QAA4B,oBAA5B;AAEA,eAAe;EACbC,UAAU,EAAE;IACVL;EADU,CADC;;EAIbM,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IADL,CAAP;EAGD,CARY;;EASbC,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,OAAO,KAAKF,QAAZ;IACD,CAHO;;IAIRG,qBAAqB,GAAG;MACtB,OAAO,KAAKD,gBAAL,CAAsBE,MAA7B;IACD,CANO;;IAOR,GAAGT,QAAQ,CAACE,WAAD,EAAc;MACvBG,QAAQ,EAAE;IADa,CAAd;EAPH,CATG;EAoBbK,OAAO,EAAE,EACP,GAAGT,UAAU,CAACC,WAAD,EAAc,CAAC,kBAAD,CAAd,CADN;;IAGP,MAAMS,gBAAN,GAAyB;MACvB,IAAI;QACF,KAAKN,QAAL,GAAgB,MAAMN,cAAc,CAACa,MAAf,EAAtB;MACF,CAFA,CAEE,OAAOC,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD;;EATM,CApBI;;EA+BbG,OAAO,GAAG;IACR,KAAKL,gBAAL;EACD;;AAjCY,CAAf","names":["Products","ProductService","mapState","mapActions","useAppStore","components","data","products","computed","filteredProducts","filteredProductsCount","length","methods","retrieveProducts","getAll","error","console","log","mounted"],"sourceRoot":"","sources":["D:\\CT449\\BaiTapLon_BanChinhThuc1\\FrontEnd\\src\\views\\ProductView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"header_product\">\n      <h2>Tất cả sản phẩm</h2>\n    </div>\n    <div class=\"tab-result\">\n      <!-- <div>\n\n      </div> -->\n      <products\n        v-if=\"filteredProductsCount > 0\"\n        :mountproduct=\"filteredProducts\"\n      >\n      </products>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Products from \"../components/ProductWape.vue\";\nimport ProductService from \"@/services/product.service\";\nimport { mapState, mapActions } from \"pinia\";\nimport { useAppStore } from \"@/stores/app.store\";\n\nexport default {\n  components: {\n    Products,\n  },\n  data() {\n    return {\n      products: [],\n    };\n  },\n  computed: {\n    filteredProducts() {\n      return this.products;\n    },\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    },\n    ...mapState(useAppStore, {\n      products: \"products\",\n    }),\n  },\n  methods: {\n    ...mapActions(useAppStore, [\"retrieveProducts\"]),\n\n    async retrieveProducts() {\n      try {\n        this.products = await ProductService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n  mounted() {\n    this.retrieveProducts();\n  },\n};\n</script>\n\n<style cope>\n.tab-result {\n  margin-top: 28px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}