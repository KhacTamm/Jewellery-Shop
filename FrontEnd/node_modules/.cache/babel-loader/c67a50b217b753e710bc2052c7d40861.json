       <th scope=\"row\"></th>\n            <td>{{product.name}}</td>\n            <td>{{product.quantity}}</td>\n            <td>{{product.BaPrice}}</td>\n            <td>{{product.PrPrice}}</td>\n            <td>\n              <router-link \n              :to =\"{ \n              path: '/admin/{{product.id}}/edit',\n              params: { id: product._id } }\" \n              class=\"btn btn-link\">\n               Sửa\n               </router-link>\n            </td>\n            <td>\n                <button @click=\"deleteProduct\">Xóa</button>\n            </td>\n        </tr>\n        <!-- {{else}} -->\n        <tr>\n          <td colspan=\"5\" class=\"text-center\">\n            Bạn chưa có khóa học nào.\n            <a href=\"/courses/create\">Tạo khóa học</a>\n\n          </td>\n        </tr>\n        <!-- {{/each}} -->\n    </tbody>\n    </table>\n</form>\n\n<!-- {{!-- comfirm delete course--}} -->\n<!-- <div class=\"modal fade\" id=\"delete-cousre-Modal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLabel\">Xóa vĩnh viễn khóa học</h5>\n        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n      </div>\n      <div class=\"modal-body\">\n        Bạn chắc chắn muốn xóa khóa học này vĩnh viễn?\n      </div>\n      <div class=\"modal-footer\">\n        <button id = \"btn-delete-course\" type=\"button\" class=\"btn btn-danger\">Xóa vĩnh viễn</button>\n        <button type=\"button\" class=\"btn btn-secondary\"  data-bs-dismiss=\"modal\">Hủy</button>\n      </div>\n    </div>\n</div> -->\n\n<!-- <form name=\"delete-course-form\" method=\"POST\"></form>\n<form name=\"restore-course-form\" method=\"POST\"></form> -->\n</template>\n\n<script>\nimport ContactService from \"@/services/contact.service\";\n\nexport default {\n  data() {\n    return {\n      products: [],\n    };\n  },\n  computed: {\n    // Trả về các contact có chứa thông tin cần tìm kiếm.\n    filteredProducts() {\n      return this.products;\n    },\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    },\n  },\n  methods: {\n    t() {\n      console.log(this.mountproduct);\n    },\n    deleteProduct() {},\n    async retrieveProducts() {\n      try {\n        this.products = await ContactService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async createProduct(data) {\n      try {\n        this.$router.push({ name: \"product\" });\n        await ContactService.create(data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n  mounted() {\n    this.retrieveProducts();\n  },\n  //   async retrieveProducts() {\n  //     try {\n  //       this.products = await ContactService.getAll();\n  //     } catch (error) {\n  //       console.log(error);\n  //     }\n  //   },\n  //   mounted() {\n  //     this.retrieveProducts();\n  //   },\n  // },\n};\n</script>\n\n<style cope>\n.tm {\n  height: 40px;\n  margin-bottom: 24px;\n  align-items: center;\n}\n\n.checkbox-select-all-options {\n  width: 150px;\n  margin: 24px;\n  /* height: 40px; */\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}