  <!-- <div v-else>\n    <products :mountproduct=\"filteredProducts\"> </products>\n  </div> -->\n</template>\n\n<script>\n// import Products from \"../components/ProductWape.vue\";\nimport { useProductData } from \"@/stores/Products\";\nimport ContactService from \"@/services/contact.service\";\n// import ContactService from \"@/assets/img/imgLocal/404.png\";\n\nexport default {\n  components: {\n    // Products,\n  },\n  data() {\n    return {\n      products: [],\n      activeIndex: -1,\n      searchText: useProductData(),\n      imge: require(\"../assets/img/imgLocal/404.png\"),\n    };\n  },\n  watch: {\n    // SearchText() {\n    //   this.activeIndex = -1;\n    // },\n  },\n  computed: {\n    productStrings() {\n      return this.products.map((product) => {\n        const { type, name } = product;\n        return [type, name].join(\"\").toLowerCase().trim();\n      });\n    },\n    // Trả về các contact có chứa thông tin cần tìm kiếm.\n    filteredProducts() {\n      return this.products.filter((contact, index) =>\n        this.productStrings[index].includes(this.searchText.SearchText)\n      );\n    },\n    activeContact() {\n      if (this.activeIndex < 0) return null;\n      return this.products[this.activeIndex];\n    },\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    },\n  },\n  methods: {\n    test() {\n      console.log(this.searchText.SearchText);\n    },\n    async retrieveContacts() {\n      try {\n        this.products = await ContactService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    refreshList() {\n      this.retrieveContacts();\n      this.activeIndex = -1;\n    },\n  },\n  mounted() {\n    this.refreshList();\n  },\n};\n</script>\n\n<style>\n.WapperSearchEmpty {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 80px;\n}\n\n.SearchEmpty {\n  text-align: center;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}