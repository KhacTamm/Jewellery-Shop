{"ast":null,"code":"import * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage\n  },\n  props: {\n    account: {\n      type: Object,\n      required: true\n    }\n  },\n\n  data() {\n    const accountFormSchema = yup.object().shape({\n      // username: yup\n      //   .string()\n      //   .required(\"Vui lòng nhập trường này.\")\n      //   .min(2, \"Vui lòng tối thiểu 2 kí tự.\")\n      //   .max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      phone: yup.string().required(\"Vui lòng nhập trường này.\").matches(/((09|03|07|08|05)+([0-9]{8})\\b)/g, \"Số điện thoại không hợp lệ.\"),\n      password: yup.string().required(\"Vui lòng nhập trường này.\").min(4, \"Mật khẩu phải dài tối thiểu 4 kí tự.\").max(8, \"Mật khẩu dài tối đa 8 kí tự.\")\n    });\n    return {\n      accountLocal: this.account,\n      accountFormSchema\n    };\n  },\n\n  methods: {\n    test() {\n      this.p = this.accountLocal.password;\n      console.log(this.p);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgDA,OAAO,KAAKA,GAAZ,MAAqB,KAArB;AACA,SAASC,IAAT,EAAeC,KAAf,EAAsBC,YAAtB,QAA0C,cAA1C;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,IADU;IAEVC,KAFU;IAGVC;EAHU,CADC;EAMbE,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAR;MAAgBC,QAAQ,EAAE;IAA1B;EADJ,CANM;;EASbC,IAAI,GAAG;IACL,MAAMC,iBAAgB,GAAIX,GAAG,CAACY,MAAJ,GAAaC,KAAb,CAAmB;MAC3C;MACA;MACA;MACA;MACA;MACAC,KAAK,EAAEd,GAAE,CACNe,MADI,GAEJN,QAFI,CAEK,2BAFL,EAGJO,OAHI,CAIH,kCAJG,EAKH,6BALG,CANoC;MAa3CC,QAAQ,EAAEjB,GAAE,CACTe,MADO,GAEPN,QAFO,CAEE,2BAFF,EAGPS,GAHO,CAGH,CAHG,EAGA,sCAHA,EAIPC,GAJO,CAIH,CAJG,EAIA,8BAJA;IAbiC,CAAnB,CAA1B;IAmBA,OAAO;MACLC,YAAY,EAAE,KAAKd,OADd;MAELK;IAFK,CAAP;EAID,CAjCY;;EAkCbU,OAAO,EAAE;IACPC,IAAI,GAAG;MACL,KAAKC,CAAL,GAAS,KAAKH,YAAL,CAAkBH,QAA3B;MACAO,OAAO,CAACC,GAAR,CAAY,KAAKF,CAAjB;IACD;;EAJM;AAlCI,CAAf","names":["yup","Form","Field","ErrorMessage","components","props","account","type","Object","required","data","accountFormSchema","object","shape","phone","string","matches","password","min","max","accountLocal","methods","test","p","console","log"],"sourceRoot":"","sources":["D:\\CT449\\shoponline\\src\\components\\AppLogin.vue"],"sourcesContent":["<template>\n  <div id=\"login\">\n    <h2 class=\"titleAccount\">Đăng Nhập</h2>\n    <Form\n      class=\"login\"\n      :validation-schema=\"accountFormSchema\"\n      @submit=\"$emit('submit:account', accountLocal)\"\n    >\n      <ul>\n        <li>\n          <Field\n            name=\"phone\"\n            type=\"text\"\n            placeholder=\"Số điện thoại\"\n            class=\"text\"\n            v-model=\"accountLocal.phone\"\n          />\n          <ErrorMessage name=\"phone\" class=\"error-feedback\" />\n        </li>\n        <li>\n          <Field\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Mật khẩu\"\n            class=\"text\"\n            v-model=\"accountLocal.password\"\n          />\n          <ErrorMessage name=\"password\" class=\"error-feedback\" />\n        </li>\n        <div class=\"rep_pas\">\n          <a href=\"\">Quên mật khẩu?</a>\n        </div>\n        <div class=\"form-login\">\n          <button type=\"submit\" class=\"login btn\">ĐĂNG NHẬP</button>\n        </div>\n      </ul>\n    </Form>\n    <div class=\"page-login__thirdPartLogin_line\">\n      <span>Hoặc</span>\n    </div>\n    <div class=\"register\">\n      Bạn chưa có tài khoản?\n      <a href=\"\">Đăng ký</a>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as yup from \"yup\";\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\n\nexport default {\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  props: {\n    account: { type: Object, required: true },\n  },\n  data() {\n    const accountFormSchema = yup.object().shape({\n      // username: yup\n      //   .string()\n      //   .required(\"Vui lòng nhập trường này.\")\n      //   .min(2, \"Vui lòng tối thiểu 2 kí tự.\")\n      //   .max(250, \"Tên có nhiều nhất 250 ký tự.\"),\n      phone: yup\n        .string()\n        .required(\"Vui lòng nhập trường này.\")\n        .matches(\n          /((09|03|07|08|05)+([0-9]{8})\\b)/g,\n          \"Số điện thoại không hợp lệ.\"\n        ),\n      password: yup\n        .string()\n        .required(\"Vui lòng nhập trường này.\")\n        .min(4, \"Mật khẩu phải dài tối thiểu 4 kí tự.\")\n        .max(8, \"Mật khẩu dài tối đa 8 kí tự.\"),\n    });\n    return {\n      accountLocal: this.account,\n      accountFormSchema,\n    };\n  },\n  methods: {\n    test() {\n      this.p = this.accountLocal.password;\n      console.log(this.p);\n    },\n  },\n};\n</script>\n\n<style>\n.titleAccount {\n  font-size: 1.4rem;\n  font-weight: 600;\n  text-align: center;\n}\n\n.login ul {\n  display: flex;\n  flex-wrap: wrap;\n  /* justify-content: center; */\n  margin: 6px;\n}\n\n.login ul li {\n  width: 100%;\n  height: 48px;\n  margin: 12px 0;\n  border: 1px solid #999999;\n}\n\n.login ul li input {\n  background-color: transparent;\n  padding: 0 6px;\n}\n.text {\n  width: 100%;\n  height: 100%;\n  padding: 6px;\n  outline: none;\n  border: none;\n  font-size: 0.9rem;\n}\n\n.form-login {\n  height: 42px;\n  margin-top: 30px;\n  width: 100%;\n}\n\n.form-login .login {\n  background-color: var(--backg);\n  width: 100%;\n  height: 100%;\n  padding: 6px;\n  font-weight: 700;\n  cursor: pointer;\n  text-align: center;\n}\n\n.form-login .login:hover {\n  opacity: 0.8;\n}\n\n.rep_pas {\n  margin-top: 6px;\n}\n\n.rep_pas a {\n  font-size: 0.9rem;\n  color: #666666;\n}\n\n.rep_pas a:hover {\n  color: #000000;\n}\n\n.page-login__thirdPartLogin_line {\n  position: relative;\n  color: #b2b2b2;\n  padding: 12px 0;\n  font-size: 1rem;\n}\n\n.page-login__thirdPartLogin_line::before {\n  content: \"\";\n  width: 44%;\n  height: 1px;\n  background: #b2b2b2;\n  position: absolute;\n  top: 50%;\n  left: 0;\n}\n\n.page-login__thirdPartLogin_line::after {\n  content: \"\";\n  width: 44%;\n  height: 1px;\n  background: #b2b2b2;\n  position: absolute;\n  top: 50%;\n  right: 0;\n}\n\n.page-login__thirdPartLogin_line span {\n  padding-left: 45%;\n}\n.register {\n  font-size: 0.9rem;\n  color: #666666;\n}\n\n.register a {\n  font-size: 1rem;\n  color: #0000ff;\n  font-weight: 700;\n}\n\n.register a:hover {\n  opacity: 0.8;\n  text-decoration-line: underline;\n}\n\n.error-feedback {\n  font-size: 0.8rem;\n  text-align: left;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}