{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AdminAdd from \"../components/AdminAdd.vue\";\nimport AdminAllProduct from \"../components/AdminAllProduct.vue\";\nimport ContactService from \"@/services/contact.service\";\nexport default {\n  components: {\n    AdminAdd,\n    AdminAllProduct\n  },\n\n  data() {\n    return {\n      product: {\n        type: Object,\n        require: true\n      },\n      products: [],\n      AddProduct: false,\n      AllProduct: false\n    };\n  },\n\n  computed: {\n    // Trả về các contact có chứa thông tin cần tìm kiếm.\n    filteredProducts() {\n      return this.products;\n    },\n\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    }\n\n  },\n  methods: {\n    displayAllProduct() {\n      console.log(this.filteredProducts);\n      this.AllProduct = !this.AllProduct;\n    },\n\n    displayAddProduct() {\n      this.AddProduct = !this.AddProduct;\n    },\n\n    async createProduct(data) {\n      try {\n        this.$router.push({\n          name: \"product\"\n        });\n        await ContactService.create(data);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async retrieveProducts() {\n      try {\n        this.products = await ContactService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    mounted() {\n      this.retrieveProducts();\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAmCA,OAAOA,QAAP,MAAqB,4BAArB;AACA,OAAOC,eAAP,MAA4B,mCAA5B;AACA,OAAOC,cAAP,MAA2B,4BAA3B;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,QADU;IAEVC;EAFU,CADC;;EAKbG,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,IAAI,EAAEC,MADC;QAEPC,OAAO,EAAE;MAFF,CADJ;MAKLC,QAAQ,EAAE,EALL;MAMLC,UAAU,EAAE,KANP;MAOLC,UAAU,EAAE;IAPP,CAAP;EASD,CAfY;;EAgBbC,QAAQ,EAAE;IACR;IACAC,gBAAgB,GAAG;MACjB,OAAO,KAAKJ,QAAZ;IACD,CAJO;;IAKRK,qBAAqB,GAAG;MACtB,OAAO,KAAKD,gBAAL,CAAsBE,MAA7B;IACD;;EAPO,CAhBG;EAyBbC,OAAO,EAAE;IACPC,iBAAiB,GAAG;MAClBC,OAAO,CAACC,GAAR,CAAY,KAAKN,gBAAjB;M