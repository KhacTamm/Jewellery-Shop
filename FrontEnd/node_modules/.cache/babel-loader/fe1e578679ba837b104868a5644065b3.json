{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"pinia\";\nimport { useAppStore } from \"@/stores/app.store\";\nimport AdminService from \"@/services/admin.service\";\nexport default {\n  data() {\n    return {\n      products: [],\n      typeProducts: []\n    };\n  },\n\n  computed: {\n    filteredProducts() {\n      return this.products;\n    },\n\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    },\n\n    filteredTypeT() {\n      return this.typeProducts;\n    }\n\n  },\n  methods: { ...mapActions(useAppStore, [\"formatter\"]),\n\n    async renderType(id) {\n      try {\n        const data = await AdminService.getTypeProducts(id);\n        this.products = data.products;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async deleteAllProduct() {\n      try {\n        this.$router.push({\n          name: \"AdminAllProduct\"\n        });\n        await AdminService.deleteAll();\n        this.$refs.form.$el.submit();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async deleteProduct(id) {\n      try {\n        await AdminService.forceDelete(id);\n        this.$router.push({\n          name: \"AdminAllProduct\"\n        });\n        this.$refs.form.$el.submit();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async retrieveProducts() {\n      try {\n        this.products = await AdminService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async retrieveTypeP() {\n      try {\n        this.typeProducts = await AdminService.getAllTypeProducts();\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n\n  mounted() {\n    this.retrieveProducts();\n    this.retrieveTypeP();\n  }\n\n};","map":{"version":3,"mappings":";AA0JA,SAASA,UAAT,QAA2B,OAA3B;AACA,SAASC,WAAT,QAA4B,oBAA5B;AACA,OAAOC,YAAP,MAAyB,0BAAzB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,YAAY,EAAE;IAFT,CAAP;EAID,CANY;;EAObC,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,OAAO,KAAKH,QAAZ;IACD,CAHO;;IAIRI,qBAAqB,GAAG;MACtB,OAAO,KAAKD,gBAAL,CAAsBE,MAA7B;IACD,CANO;;IAORC,aAAa,GAAG;MACd,OAAO,KAAKL,YAAZ;IACD;;EATO,CAPG;EAkBbM,OAAO,EAAE,EACP,GAAGX,UAAU,CAACC,WAAD,EAAc,CAAC,WAAD,CAAd,CADN;;IAGP,MAAMW,UAAN,CAAiBC,EAAjB,EAAqB;MACnB,IAAI;QACF,MAAMV,IAAG,GAAI,MAAMD,YAAY,CAACY,eAAb,CAA6BD,EAA7B,CAAnB;QACA,KAAKT,QAAL,GAAgBD,IAAI,CAACC,QAArB;MACF,CAHA,CAGE,OAAOW,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD,CAVM;;IAYP,MAAMG,gBAAN,GAAyB;MACvB,IAAI;QACF,KAAKC,OAAL,CAAaC,IAAb,CAAkB;UAAEC,IAAI,EAAE;QAAR,CAAlB;QACA,MAAMnB,YAAY,CAACoB,SAAb,EAAN;QACA,KAAKC,KAAL,CAAWC,IAAX,CAAgBC,GAAhB,CAAoBC,MAApB;MACF,CAJA,CAIE,OAAOX,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD,CApBM;;IAsBP,MAAMY,aAAN,CAAoBd,EAApB,EAAwB;MACtB,IAAI;QACF,MAAMX,YAAY,CAAC0B,WAAb,CAAyBf,EAAzB,CAAN;QACA,KAAKM,OAAL,CAAaC,IAAb,CAAkB;UAAEC,IAAI,EAAE;QAAR,CAAlB;QACA,KAAKE,KAAL,CAAWC,IAAX,CAAgBC,GAAhB,CAAoBC,MAApB;MACF,CAJA,CAIE,OAAOX,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD,CA9BM;;IAgCP,MAAMc,gBAAN,GAAyB;MACvB,IAAI;QACF,KAAKzB,QAAL,GAAgB,MAAMF,YAAY,CAAC4B,MAAb,EAAtB;MACF,CAFA,CAEE,OAAOf,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD,CAtCM;;IAwCP,MAAMgB,aAAN,GAAsB;MACpB,IAAI;QACF,KAAK1B,YAAL,GAAoB,MAAMH,YAAY,CAAC8B,kBAAb,EAA1B;MACF,CAFA,CAEE,OAAOjB,KAAP,EAAc;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACF;IACD;;EA9CM,CAlBI;;EAkEbkB,OAAO,GAAG;IACR,KAAKJ,gBAAL;IACA,KAAKE,aAAL;EACD;;AArEY,CAAf","names":["mapActions","useAppStore","AdminService","data","products","typeProducts","computed","filteredProducts","filteredProductsCount","length","filteredTypeT","methods","renderType","id","getTypeProducts","error","console","log","deleteAllProduct","$router","push","name","deleteAll","$refs","form","$el","submit","deleteProduct","forceDelete","retrieveProducts","getAll","retrieveTypeP","getAllTypeProducts","mounted"],"sourceRoot":"","sources":["D:\\CT449\\BaiTapLon_BanChinhThuc1\\FrontEnd\\src\\components\\AdminAllProduct.vue"],"sourcesContent":["<template>\n  <div class=\"titileAdminView\">\n    <h3>Tất cả sản phẩm</h3>\n  </div>\n  <div class=\"wapperAdmin\">\n    <div class=\"NoEmpty\" v-if=\"filteredProductsCount > 0\">\n      <div class=\"mb-3 type\">\n        <label for=\"type\" class=\"form-label\">Phân loại</label>\n        <select\n          class=\"form-select form-select-sm checkbox-select-all-options\"\n          name=\"action\"\n          required\n          ref=\"type\"\n          @click=\"addType\"\n        >\n          <option>Chọn loại sản phẩm</option>\n          <option\n            v-for=\"(item, index) in filteredTypeT\"\n            :key=\"index\"\n            :value=\"`${item._id}`\"\n            @click=\"renderType(item._id)\"\n          >\n            {{ item.name }}\n          </option>\n        </select>\n      </div>\n      <!-- <div class=\"typeP\">\n        <div\n          v-for=\"(item, index) in filteredTypeT\"\n          :key=\"index\"\n          class=\"typeP_item\"\n          @click=\"renderType(item._id)\"\n        >\n          {{ item.name }}\n        </div>\n      </div> -->\n      <form class=\"mt-4\" name=\"container-form\">\n        <table class=\"table\">\n          <thead>\n            <tr class=\"header_allProduct\">\n              <th scope=\"col\" class=\"allProduct_sp\">Sản phẩm</th>\n              <th scope=\"col\">Kho</th>\n              <th scope=\"col\">Giá Gốc</th>\n              <th scope=\"col\">Giá Khuyết Mãi</th>\n              <th scope=\"col\">Sửa</th>\n              <th scope=\"col\">Xóa</th>\n            </tr>\n          </thead>\n          <tbody class=\"body_allProduct\">\n            <tr v-for=\"(product, item) in filteredProducts\" :key=\"item\">\n              <td class=\"titleProduct\">\n                <img\n                  class=\"imgProductAdmin\"\n                  :src=\"`${require('@/assets/img/product/' + product.img[0])}`\"\n                />\n                <div>\n                  <router-link\n                    :to=\"{\n                      name: 'productdetail',\n                      params: { id: product._id },\n                    }\"\n                  >\n                    {{ product.name }}\n                  </router-link>\n                </div>\n              </td>\n              <td>{{ product.quantity }}</td>\n              <td>{{ formatter(`${product.BaPrice}`) }}</td>\n              <td>{{ formatter(`${product.PrPrice}`) }}</td>\n              <td>\n                <router-link\n                  :to=\"{\n                    name: 'AdminEditProduct',\n                    params: { id: product._id },\n                  }\"\n                >\n                  <button class=\"btns button_update\">Sửa</button>\n                </router-link>\n              </td>\n              <td>\n                <button\n                  class=\"btns button_delete\"\n                  @mousedown=\"deleteProduct(`${product._id}`)\"\n                >\n                  Xóa\n                </button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n        <div class=\"deleteAll\">\n          <div\n            class=\"btns button_delete_all\"\n            data-bs-toggle=\"modal\"\n            data-bs-target=\"#delete-productAll-Modal\"\n            v-on:click.prevent\n          >\n            Xóa tất cả sản phẩm\n          </div>\n          <!-- {{!-- comfirm delete course--}} -->\n          <div\n            class=\"modal fade\"\n            id=\"delete-productAll-Modal\"\n            tabindex=\"-1\"\n            aria-labelledby=\"exampleModalLabel\"\n            aria-hidden=\"true\"\n          >\n            <div class=\"modal-dialog\">\n              <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                  <h5 class=\"modal-title\" id=\"exampleModalLabel\">\n                    Xóa sản phẩm\n                  </h5>\n                  <button\n                    type=\"button\"\n                    class=\"btn-close\"\n                    data-bs-dismiss=\"modal\"\n                    aria-label=\"Close\"\n                  ></button>\n                </div>\n                <div class=\"modal-body\">Bạn có muốn xóa tất cả sản phẩm?</div>\n                <div class=\"modal-footer\">\n                  <button\n                    ref=\"form\"\n                    id=\"btn-delete-course\"\n                    class=\"btns button_comfirm\"\n                    @click=\"deleteAllProduct\"\n                  >\n                    Xác nhận\n                  </button>\n                  <button\n                    type=\"button\"\n                    class=\"btns button_update\"\n                    data-bs-dismiss=\"modal\"\n                  >\n                    Hủy\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </form>\n    </div>\n    <div v-else class=\"emptyProduct\">\n      Bạn chưa có sản phẩm nào.\n      <router-link :to=\"{ name: 'AdminAdd' }\">\n        <span class=\"textGoToAdminAdd\">Thêm sản phẩm</span>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from \"pinia\";\nimport { useAppStore } from \"@/stores/app.store\";\nimport AdminService from \"@/services/admin.service\";\n\nexport default {\n  data() {\n    return {\n      products: [],\n      typeProducts: [],\n    };\n  },\n  computed: {\n    filteredProducts() {\n      return this.products;\n    },\n    filteredProductsCount() {\n      return this.filteredProducts.length;\n    },\n    filteredTypeT() {\n      return this.typeProducts;\n    },\n  },\n  methods: {\n    ...mapActions(useAppStore, [\"formatter\"]),\n\n    async renderType(id) {\n      try {\n        const data = await AdminService.getTypeProducts(id);\n        this.products = data.products;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async deleteAllProduct() {\n      try {\n        this.$router.push({ name: \"AdminAllProduct\" });\n        await AdminService.deleteAll();\n        this.$refs.form.$el.submit();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async deleteProduct(id) {\n      try {\n        await AdminService.forceDelete(id);\n        this.$router.push({ name: \"AdminAllProduct\" });\n        this.$refs.form.$el.submit();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async retrieveProducts() {\n      try {\n        this.products = await AdminService.getAll();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async retrieveTypeP() {\n      try {\n        this.typeProducts = await AdminService.getAllTypeProducts();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n  mounted() {\n    this.retrieveProducts();\n    this.retrieveTypeP();\n  },\n};\n</script>\n\n<style>\n.titileAdminView h3 {\n  background-color: rgba(216, 216, 216, 0.8);\n  padding: 12px 24px;\n}\n\n.wapperAdmin {\n  padding: 0 24px;\n}\n\n.mt-4 {\n  width: 100%;\n}\n\n.tm {\n  height: 40px;\n  margin-bottom: 24px;\n  align-items: center;\n}\n\n.typeP {\n  display: flex;\n  /* padding: 24px; */\n  cursor: pointer;\n}\n\n.typeP_item {\n  padding: 16px 12px 0;\n  font-weight: 650;\n  text-decoration: underline;\n  color: var(--text-color);\n}\n\n.typeP_item:hover {\n  opacity: 0.6;\n}\n.header_allProduct th {\n  text-align: center;\n}\n\n.body_allProduct tr {\n  /* display: table-cell; */\n  vertical-align: middle;\n}\n\n.body_allProduct td:not(td:first-child) {\n  text-align: center;\n}\n\n.titleProduct {\n  display: flex;\n  align-items: center;\n}\n\n.imgProductAdmin {\n  width: 120px;\n  object-fit: cover;\n  padding-right: 12px;\n}\n\n.allProduct_sp {\n  width: 480px;\n}\n\n.btns {\n  -webkit-transition-duration: 0.4s; /* Safari */\n  transition-duration: 0.4s;\n  font-weight: 600;\n}\n\n.btns.button_delete {\n  border: 2px solid crimson;\n  background-color: crimson;\n  color: white;\n}\n\n.btns.button_update {\n  border: 2px solid var(--backg);\n  color: #000;\n  background-color: var(--backg);\n}\n\n.btns.button_comfirm {\n  border: 3px solid var(--backg);\n  background-color: transparent;\n}\n\n.btns:hover {\n  opacity: 0.8;\n}\n\n.modal.show .modal-dialog {\n  display: flex;\n  justify-content: center;\n  top: 35%;\n}\n\n.modal-content {\n  width: 360px;\n}\n\n.deleteAll {\n  width: 100%;\n  margin-top: 24px;\n  text-align: right;\n}\n\n.fade {\n  text-align: center;\n}\n\n.button_delete_all {\n  float: right;\n  font-size: 1.2rem;\n  font-weight: 400;\n  cursor: pointer;\n  color: var(--text-color);\n  text-decoration: underline;\n}\n\n.emptyProduct {\n  font-size: 1.2rem;\n  padding: 24px 0;\n}\n\n.textGoToAdminAdd {\n  font-weight: 600;\n  color: var(--text-color);\n  text-decoration: underline;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}