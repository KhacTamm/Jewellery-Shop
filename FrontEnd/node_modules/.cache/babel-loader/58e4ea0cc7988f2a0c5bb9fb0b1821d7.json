{"ast":null,"code":"import { mapState, mapActions } from \"pinia\";\nimport { useAppStore } from \"@/stores/app.store\";\nimport CartProduct from \"@/components/CartProduct.vue\";\nimport CartReceipt from \"@/components/CartReceipt.vue\";\nimport CartEmpty from \"@/components/CartEmpty.vue\";\nimport CartService from \"@/services/cart.service\";\nexport default {\n  components: {\n    CartProduct,\n    CartReceipt,\n    CartEmpty\n  },\n  computed: { ...mapState(useAppStore, {\n      products: \"products\"\n    })\n  },\n  methods: { ...mapActions(useAppStore, [\"formatter\", \"getCartStore\", \"removeCartAllStore\"]),\n\n    async getCart() {\n      await this.getCartStore();\n    },\n\n    async removeCartAll() {\n      await CartService.removeCartAll();\n    }\n\n  },\n\n  mounted() {\n    this.getCart();\n  }\n\n};","map":{"version":3,"mappings":"AAiCA,SAASA,QAAT,EAAmBC,UAAnB,QAAqC,OAArC;AACA,SAASC,WAAT,QAA4B,oBAA5B;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AACA,OAAOC,WAAP,MAAwB,yBAAxB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVJ,WADU;IAEVC,WAFU;IAGVC;EAHU,CADC;EAMbG,QAAQ,EAAE,EACR,GAAGR,QAAQ,CAACE,WAAD,EAAc;MACvBO,QAAQ,EAAE;IADa,CAAd;EADH,CANG;EAWbC,OAAO,EAAE,EACP,GAAGT,UAAU,CAACC,WAAD,EAAc,CACzB,WADyB,EAEzB,cAFyB,EAGzB,oBAHyB,CAAd,CADN;;IAOP,MAAMS,OAAN,GAAgB;MACd,MAAM,KAAKC,YAAL,EAAN;IACD,CATM;;IAWP,MAAMC,aAAN,GAAsB;MACpB,MAAMP,WAAW,CAACO,aAAZ,EAAN;IACD;;EAbM,CAXI;;EA0BbC,OAAO,GAAG;IACR,KAAKH,OAAL;EACD;;AA5BY,CAAf","names":["mapState","mapActions","useAppStore","CartProduct","CartReceipt","CartEmpty","CartService","components","computed","products","methods","getCart","getCartStore","removeCartAll","mounted"],"sourceRoot":"","sources":["D:\\CT449\\BaiTapLon\\FrontEnd\\src\\views\\CartView.vue"],"sourcesContent":["<template>\n  <div class=\"cart\">\n    <div v-if=\"products.totalPriceCart > 0\" class=\"CartNp\">\n      <div class=\"wrap_cl1\">\n        <div class=\"cl1\">\n          <div class=\"header_cl1\">\n            <h2 cla